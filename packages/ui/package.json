    "next-themes": "~0.3.0",
    "tailwind-merge": "~2.5.4",
    "tailwindcss": "~3.4.14",
    "tailwindcss-animate": "~1.0.7",
    "vaul": "~1.1.0",
    "zod": "~3.23.8"
  },
  "devDependencies": {
    "@acme/typescript-config": "workspace:~",
    "@types/ms": "~0.7.34",
    "@types/node": "~22.7.9",
    "@types/react": "~18.3.12",
    "@types/react-dom": "~18.3.1",
    "react": "~18.3.1",
    "typescript": "~5.6.3"
  }
